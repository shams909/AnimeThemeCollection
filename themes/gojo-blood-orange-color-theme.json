    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#FF8A65",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#FFC107"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF3D00"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#FF7043",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#FBE9E7"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#FF9800"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FF5252"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#FF3D00"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#FFAB91"
            }
        }
    ]
}