    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#737373",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#4ADE80"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#E11D48"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#F43F5E",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#F5F5F5"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#FDA4AF"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#FB7185"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#FCA5A5"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#86EFAC"
            }
        }
    ]
}