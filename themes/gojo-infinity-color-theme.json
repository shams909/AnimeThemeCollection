    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6B7280",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#059669"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#9333EA"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#60A5FA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#CBD5E1"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#F59E0B"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#F87171"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#9333EA"
            }
        },
        {
            "name": "Classes & Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#0891B2"
            }
        },
        {
            "name": "Properties",
            "scope": [
                "variable.other.property",
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#E2E8F0"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml"
            ],
            "settings": {
                "foreground": "#9333EA"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#FBBF24"
            }
        },
        {
            "name": "Method Declaration",
            "scope": [
                "meta.method.declaration"
            ],
            "settings": {
                "foreground": "#60A5FA"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#9CA3AF"
            }
        }
    ]
}