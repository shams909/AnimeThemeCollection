    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#64748B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#A7F3D0"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#38BDF8"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#7DD3FC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#F1F5F9"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#FCA5A5"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#EF4444"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#BAE6FD"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#E2E8F0"
            }
        }
    ]
}