    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#4F46E5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#06B6D4"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#A855F7"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#FBBF24",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.definition.variable.name",
                "support.variable"
            ],
            "settings": {
                "foreground": "#EEF2FF"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#EC4899"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#F472B6"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#818CF8"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#38BDF8"
            }
        }
    ]
}